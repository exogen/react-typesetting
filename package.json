{
  "name": "react-typesetting",
  "version": "0.0.0",
  "main": "index.js",
  "author": "Brian Beck <exogen@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "npm run build:demo && npm run build:dist && npm run build:docs",
    "build:demo": "rimraf .next docs && next build && next export -o docs",
    "build:dist": "npm run build:dist-esm && npm run build:dist-lib",
    "build:dist-esm": "BABEL_ENV=esm babel src -d dist-esm",
    "build:dist-lib": "babel src -d dist-lib",
    "build:docs": "md-magic README.md",
    "start:dev": "parcel serve demo/index.html"
  },
  "peerDependencies": {
    "react": "^16.3.0",
    "react-dom": "^16.3.0"
  },
  "dependencies": {
    "debug": "^4.0.1",
    "fontfaceobserver": "^2.0.13",
    "prop-types": "^15.6.2",
    "react-resize-observer": "^1.0.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "babel-plugin-styled-components": "^1.8.0",
    "execa": "^1.0.0",
    "jest": "^23.6.0",
    "markdown-magic": "^0.1.25",
    "next": "^7.0.1",
    "prettier": "^1.14.3",
    "react": "^16.5.2",
    "react-docgen": "^2.21.0",
    "react-docgen-displayname-handler": "^2.1.1",
    "react-dom": "^16.5.2",
    "react-draggable": "^3.0.5",
    "react-testing-library": "^5.2.0",
    "rimraf": "^2.6.2",
    "styled-components": "^4.0.0-beta.9",
    "webpack": "^4.20.2"
  }
}
