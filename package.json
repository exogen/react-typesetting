{
  "name": "react-typesetting",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "babel -d lib src",
    "deploy": "now -e NODE_ENV=production --name=react-typesetting",
    "deploy:link": "now ln $(pbpaste) react-typesetting",
    "lint": "standard --verbose | snazzy",
    "lint:fix": "standard --verbose --fix",
    "now-build": "next build",
    "start": "next start",
    "start:dev": "next",
    "test": "karma start --single-run",
    "test:watch": "karma start"
  },
  "dependencies": {
    "fontfaceobserver": "^2.0.9",
    "jquery": "^3.2.1",
    "next": "^2.0.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-draggable": "^2.2.3",
    "react-measure": "^1.4.6",
    "styled-components": "^1.4.4",
    "typeset": "^0.2.2"
  },
  "devDependencies": {
    "ava": "^0.18.2",
    "babel-cli": "^6.24.0",
    "babel-eslint": "^7.2.1",
    "babel-loader": "^6.4.1",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-2": "^6.22.0",
    "chai": "^3.5.0",
    "enzyme": "^2.8.0",
    "karma": "^1.5.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-mocha": "^1.3.0",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.3",
    "mocha": "^3.2.0",
    "nyc": "^10.1.2",
    "react-addons-test-utils": "^15.4.2",
    "snazzy": "^6.0.0",
    "standard": "^9.0.2",
    "webpack": "^2.3.2"
  },
  "standard": {
    "parser": "babel-eslint"
  }
}
